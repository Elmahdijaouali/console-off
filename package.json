{
  "name": "console-off",
  "version": "1.0.0",
  "description": "Disable console methods in production for any JavaScript/TypeScript project",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsup",
    "prepublishOnly": "pnpm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "console",
    "production",
    "disable",
    "react",
    "nextjs",
    "express",
    "vue",
    "angular",
    "nodejs",
    "typescript",
    "javascript",
    "logger",
    "clean"
  ],
  "author": "El Mahdi Jaouali <elmahdi@jaouali.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Elmahdijaouali/console-off"
  },
  "browser": {
    "process": false
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0"
  }
}
